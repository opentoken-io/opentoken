{
    "account": {
        "accountDir": "account/",
        "accountIdLength": 24,
        "completeLifetime": {
            "months": 6
        },
        "idHash": {
            "algorithm": "sha256",
            "hashLength": 24,
            "iterations": 10000,
            "salt": ""
        },
        "initiateLifetime": {
            "hours": 1
        },
        "passwordSaltLength": 128,
        "registrationDir": "registration/",
        "registrationIdLength": 24
    },
    "debug": false,
    "encryption": {
        "primary": {
            "cipher": {
                "algorithm": "aes-256-cbc",
                "digest": "sha512",
                "iterations": 10000
            },
            "hmac": {
                "algorithm": "sha512",
                "digest": "sha512",
                "iterations": 10000
            }
        },
        "secondary": {
            "cipher": {
                "algorithm": "bf-cbc",
                "digest": "whirlpool",
                "iterations": 10000
            },
            "hmac": {
                "algorithm": "whirlpool",
                "digest": "whirlpool",
                "iterations": 10000
            }
        }
    },
    "hotp": {
        "name": "OpenToken.io",
        "keySize": 128
    },
    "record": {
        "expirationMaximum": {
            "months": 6
        },
        "encryptionKeyFile": "encryption.key"
    },
    "schemaPath": "./schema",
    "server": {
        "baseUrl": "https://localhost:8080/",
        "name": "OpenToken.io",
        "port": 8443,
        "profileMiddleware": false,
        "proxyProtocol": false
    },
    "storage": {
        "engine": "s3",
        "s3": {
            "bucket": "fill-this-in",
            "region": "us-east-1"
        }
    }
}
